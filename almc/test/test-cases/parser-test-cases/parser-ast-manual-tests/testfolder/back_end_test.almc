fnc from("msvcrt", C) 
	printf(fmt: char*, ...) : void;

fnc from("msvcrt", C) 
	scanf(fmt: char*, ...) : void;

struct LPSYSTEMTIME {
     wYear : u16;
     wMonth : u16;
     wDayOfWeek : u16;
     wDay : u16;
     wHour : u16;
     wMinute : u16;
     wSecond : u16;
     wMilliseconds : u16;
}

fnc from("kernel32", STDCALL)
	GetLocalTime(_s: LPSYSTEMTIME*) : void;

fnc entry main1() : void {
	let a: LPSYSTEMTIME;
	GetLocalTime(&a);

	printf("day: %i, month: %i", 
		a.wDay, a.wMonth);
}

