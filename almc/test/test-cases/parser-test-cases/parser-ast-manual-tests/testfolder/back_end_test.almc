fnc relative_expr_test() : void {
	let a: i32 = 2;
	let b: i32 = 5;

	printf("%d < %d = %d\n", a, b, a < b);
	printf("%d < %d = %d\n", b, a, b < a);

	printf("%d > %d = %d\n", a, b, a > b);
	printf("%d > %d = %d\n", b, a, b > a);

	printf("%d >= %d = %d\n", a, b, a >= b);
	printf("%d >= %d = %d\n", b, a, b >= a);

	printf("%d <= %d = %d\n", a, b, a <= b);
	printf("%d <= %d = %d\n", b, a, b <= a);

	printf("%d == %d = %d\n", a, b, a == b);
	printf("%d == %d = %d\n", b, a, b == a);

	printf("%d != %d = %d\n", a, b, a != b);
	printf("%d != %d = %d\n", b, a, b != a);

	printf("%d || %d = %d\n", a, b, a || b);
	printf("%d || %d = %d\n", b, a, b || a);

	printf("%d && %d = %d\n", a, b, a && b);
	printf("%d && %d = %d\n", b, a, b && a);
}<#

